{
  "version": 2,
  "builds": [
    {
      "src": "docs/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://fixlo-backend.onrender.com/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/docs/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
