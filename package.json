{
  "name": "fixlo",
  "version": "3.0.0",
  "description": "A full-stack web application connecting homeowners with verified handyman professionals",
  "private": true,
  "workspaces": [
    "server",
    "fixlo-app"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "npm run start:frontend",
    "start:frontend": "react-scripts start",
    "start:server": "cd server && npm start",
    "start:mobile": "cd fixlo-app && npm start",
    "build": "npm run build:frontend",
    "build:frontend": "react-scripts build",
    "build:server": "cd server && npm install",
    "build:mobile": "cd fixlo-app && npm install",
    "test": "react-scripts test",
    "test:server": "cd server && npm test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:server\"",
    "dev:frontend": "react-scripts start",
    "dev:server": "cd server && npm run dev",
    "install:all": "cd server && npm install && cd ../fixlo-app && npm install",
    "clean": "rm -rf node_modules server/node_modules fixlo-app/node_modules",
    "clean:build": "rm -rf build server/dist"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "axios": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "fixlo",
    "handyman",
    "react",
    "nodejs",
    "mongodb",
    "full-stack",
    "monorepo"
  ],
  "author": "Fixlo Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Walter905-creator/fixloapp.git"
  },
  "homepage": "https://fixloapp.com"
}
