{
  "name": "fixlo",
  "version": "1.0.0",
  "description": "A full-stack web application connecting homeowners with verified handyman professionals",
  "scripts": {
    "start": "cd server && NODE_ENV=production npm start",
    "build": "npm install && cd server && npm install && REACT_APP_API_URL=https://fixloapp.onrender.com npm run build:client && cd .. && rm -rf public build && mkdir -p public build && cp -r server/client-build/* public/ && cp -r server/client-build/* build/",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd server && npm run dev",
    "client:dev": "react-scripts start",
    "install:all": "npm install && cd server && npm install",
    "client:build": "react-scripts build",
    "client:test": "react-scripts test",
    "client:eject": "react-scripts eject"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "axios": "^1.10.0",
    "concurrently": "^8.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": ["fixlo", "handyman", "connect", "react", "nodejs", "mongodb"],
  "author": "",
  "license": "MIT"
}
