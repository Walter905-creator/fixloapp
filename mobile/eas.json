{
  "cli": {
    "version": ">= 16.18.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "image": "latest",
        "resourceClass": "m-medium",
        "cocoapods": "1.15.2"
      },
      "env": {
        "EXPO_DOCTOR": "skip"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
