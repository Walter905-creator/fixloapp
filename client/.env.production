# Production environment configuration for Fixlo
REACT_APP_API_URL=https://fixloapp.onrender.com
REACT_APP_ENABLE_ANALYTICS=true

# Feature flags - ensure all features are enabled in production
REACT_APP_FEATURE_SHOW_BADGES=true
REACT_APP_FEATURE_SHARE_PROFILE=true
REACT_APP_FEATURE_BOOST_INDICATOR=true
REACT_APP_FEATURE_REVIEW_CAPTURE=true
REACT_APP_FEATURE_CLOUDINARY_UPLOAD=true

# Build optimization
GENERATE_SOURCEMAP=false