{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "build",
  "framework": null,
  "trailingSlash": false,
  "cleanUrls": true,
  "rewrites": [
    { "source": "/api/og", "destination": "/api/og" },
    { "source": "/api/meta", "destination": "/api/meta" },
    { "source": "/api/(.*)", "destination": "https://fixloapp.onrender.com/api/$1" },
    { "source": "/(.*)", "destination": "/" }
  ]
}