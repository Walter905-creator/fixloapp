{
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://fixloapp.onrender.com/api/$1" },
    { "source": "/(.*)", "destination": "/" }
  ]
}